class Bacon {
  eat() {
    print "Crunch crunch crunch!";
  }
}

Bacon().eat();

class Cake {
  init(ingredient) {
    this.ingredient = ingredient;
  }

  good() {
    return "crazily";
  }

  taste() {
    var adjective = "delicious";
    print "The " + this.flavor + " cake is " + this.good() + " " + adjective + " thanks to " + this.ingredient + "!";
  }
}

var cake = Cake("lemon");
cake.flavor = "German chocolate";
cake.taste();